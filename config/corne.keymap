&lt {
	flavor = "hold-preferred";
	tapping-term-ms = <400>;
	quick-tap-ms = <400>;
	// global-quick-tap;
};

/ {
	behaviors {
		ht: hold_tap_kp_kp {
			label = "hold_tap";
			compatible = "zmk,behavior-hold-tap";
			#binding-cells = <2>;
			flavor = "balanced";
			// flavor = "hold-preferred";
			tapping-term-ms = <400>;
			quick-tap-ms = <400>;
			// global-quick-tap;
			bindings = <&kp>, <&kp>;
		};
	};

	macros {
		macro_dot_key: macro_dot_key {
			compatible = "zmk,behavior-macro";
			#binding-cells = <0>;
			wait-ms = <40>;
			tap-ms = <40>;
			bindings
				= <&kp KP_NUM>
				, <&kp KP_DOT>
				, <&kp CLEAR2>
				;
		};
	};

